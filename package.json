    "//": [
        "Scripts Help",
        "start                  : runs the frontend",
        "server                 : runs the backend",
        "dev                    : runs both",
        "dev-nobanner           : runs both but with ephemeral set to false",
        "test-frontend          : runs frontend tests with live watch enabled",
        "test-frontend-coverage : runs frontend tests without watch and generates a coverage report",
        "test-backend           : runs backend tests with live watch enabled",
        "test-backend-coverage  : runs backend tests without watch and generates a coverage report",
        "merge-coverage         : merges the outputs of both coverage reports into one file",
        "build-frontend         : builds the frontend",
        "build-backend          : builds the backend"
    ],
    "scripts": {
        "start": "react-scripts start",
        "server": "nodemon --ignore 'backing_files_pacd_ephemeral/' --ignore 'backing_files_pacd/' --ignore 'backing_files_pacd_test*/' ./server/backend.js --port=3001 --database=pacd-ephemeral --backing-file-destination=./backing_files_pacd_ephemeral/",
        "dev": "bash -c 'BROWSER=none REACT_APP_EPHEMERAL=true REACT_APP_LATEST_COMMIT_HASH=$(git rev-parse --short HEAD) run-p server start'",
        "dev-nobanner": "bash -c 'BROWSER=none REACT_APP_LATEST_COMMIT_HASH=$(git rev-parse --short HEAD) run-p server start'",
        "test-frontend": "bash -c 'DEBUG_PRINT_LIMIT=20000 react-scripts test --watch --env=jsdom-fourteen'",
        "test-frontend-coverage": "bash -c 'react-scripts test --coverage --watchAll=false --env=jsdom-fourteen --coverageDirectory=coverage/jest-frontend'",
        "test-backend": "(bash -c 'export REACT_APP_LATEST_COMMIT_HASH=$(git rev-parse --short HEAD) && echo \"CREATE DATABASE IF NOT EXISTS \\`pacd-test-${REACT_APP_LATEST_COMMIT_HASH}\\`\" | mysql && cd server/ && npm run test -- --watch --runInBand')",
        "test-backend-coverage": "(bash -c 'export REACT_APP_LATEST_COMMIT_HASH=$(git rev-parse --short HEAD) && echo \"CREATE DATABASE IF NOT EXISTS \\`pacd-test-${REACT_APP_LATEST_COMMIT_HASH}\\`\" | mysql && cd server/ && npm run test -- --coverage --watchAll=false --runInBand --coverageDirectory=../coverage/jest-backend')",
        "merge-coverage": "cobertura-merge -o coverage/jest/cobertura-coverage.xml package1=coverage/jest-frontend/cobertura-coverage.xml package2=coverage/jest-backend/cobertura-coverage.xml",
        "build-frontend": "react-scripts build",
        "build-backend": "./node_modules/.bin/webpack --config webpack.config.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "https://localhost:3001",
    "devDependencies": {
        "babel-plugin-transform-class-properties": "^6.24.1",
        "dotenv-webpack": "^2.0.0",
        "msw": "^0.20.5",
        "raw.macro": "^0.4.2",
        "run-p": "0.0.0",
        "webpack-cli": "^3.3.12"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.js"
        ],
        "coverageReporters": [
            "text",
            "cobertura"
        ]
    }
}
